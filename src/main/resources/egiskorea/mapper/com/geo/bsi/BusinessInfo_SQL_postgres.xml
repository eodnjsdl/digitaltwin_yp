<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="businessInfo">
	
	<select id="selectBusinessInfo" parameterType="businessInfoVO" resultType="businessInfo">
		SELECT * 
		FROM YP_BSSH_INFO
		WHERE PNU = #{pnu}
	</select>
</mapper>