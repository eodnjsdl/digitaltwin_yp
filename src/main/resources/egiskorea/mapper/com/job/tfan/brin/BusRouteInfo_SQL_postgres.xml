<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="busRouteInformationDAO">
	
	<!-- 특정 정류소경유노선 조회 쿼리 -->
	<select id="getTbdThrghRouteInfoById" parameterType="String" resultType="egiskorea.com.job.tfan.brin.service.TbdThrghRouteInfoVO">
	    SELECT 	
	    		route_ty_nm,
	    		route_nm,
	    		cdpnt_sttn_nm,
	    		tmnl_sttn_nm
	    FROM 	tbd_thrgh_route_info
	    WHERE 	sttn_id = #{sttn_id}
	</select>
	
</mapper>